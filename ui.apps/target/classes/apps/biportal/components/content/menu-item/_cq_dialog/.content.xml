<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Menu Item"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
        	<menuitem
                jcr:primaryType="nt:unstructured"
                jcr:title="Menu Item Label"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                        	<menuitemlabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Menu Item Header Text"
                                fieldLabel="Menu Item Label"
                                required="{Boolean}true"
                                name="./menuItemLabel"/>
                       </items>
                    </columns>
                </items>
            </menuitem>
            <leftsectioncolumn
                jcr:primaryType="nt:unstructured"
                jcr:title="Left Section Column"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">                           
                            <leftsectionlabel
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Section label for the left column"
                                fieldLabel="Left Section Label"
                                name="./leftSectionLabel"/>
                            <leftsectionlink1label
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Plese enter a label"
                                fieldLabel="Link 1 Label"
                                name="./leftSectionLink1Label"/>
                            <leftsectionlink1url
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Please enter a destination url"
                                fieldLabel="Link 1 URL"
                                name="./leftSectionLink1URL"/>
                            <leftsectionlink2label
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Plese enter a label"
                                fieldLabel="Link 2 Label"
                                name="./leftSectionLink2Label"/>
                            <leftsectionlink2url
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Please enter a destination url"
                                fieldLabel="Link 2 URL"
                                name="./leftSectionLink2URL"/>
                            <leftsectionlink3label
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Plese enter a label"
                                fieldLabel="Link 3 Label"
                                name="./leftSectionLink3Label"/>
                            <leftsectionlink3url
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Please enter a destination url"
                                fieldLabel="Link 3 URL"
                                name="./leftSectionLink3URL"/>
                            <leftsectionlink4label
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Plese enter a label"
                                fieldLabel="Link 4 Label"
                                name="./leftSectionLink4Label"/>
                            <leftsectionlink4url
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Please enter a destination url"
                                fieldLabel="Link 4 URL"
                                name="./leftSectionLink4URL"/>
                            <leftsectionlink5label
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Plese enter a label"
                                fieldLabel="Link 5 Label"
                                name="./leftSectionLink5Label"/>
                            <leftsectionlink5url
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Please enter a destination url"
                                fieldLabel="Link 5 URL"
                                name="./leftSectionLink5URL"/>
                            <leftsectionlink6label
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Plese enter a label"
                                fieldLabel="Link 6 Label"
                                name="./leftSectionLink6Label"/>
                            <leftsectionlink6url
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Please enter a destination url"
                                fieldLabel="Link 6 URL"
                                name="./leftSectionLink6URL"/>
                        </items>
                    </columns>
                </items>
            </leftsectioncolumn>
            <rightsectioncolumn
                jcr:primaryType="nt:unstructured"
                jcr:title="Right Section Column"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <rightsectionlabel
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Section label for the right column"
                                fieldLabel="Right Section Label"
                                name="./rightSectionLabel"/>
                            <rightsectionlink1label
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Plese enter a label"
                                fieldLabel="Link 1 Label"
                                name="./rightSectionLink1Label"/>
                            <rightsectionlink1url
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Please enter a destination url"
                                fieldLabel="Link 1 URL"
                                name="./rightSectionLink1URL"/>
                            <rightsectionlink2label
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Plese enter a label"
                                fieldLabel="Link 2 Label"
                                name="./rightSectionLink2Label"/>
                            <rightsectionlink2url
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Please enter a destination url"
                                fieldLabel="Link 2 URL"
                                name="./rightSectionLink2URL"/>
                            <rightsectionlink3label
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Plese enter a label"
                                fieldLabel="Link 3 Label"
                                name="./rightSectionLink3Label"/>
                            <rightsectionlink3url
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Please enter a destination url"
                                fieldLabel="Link 3 URL"
                                name="./rightSectionLink3URL"/>
                            <rightsectionlink4label
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Plese enter a label"
                                fieldLabel="Link 4 Label"
                                name="./rightSectionLink4Label"/>
                            <rightsectionlink4url
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Please enter a destination url"
                                fieldLabel="Link 4 URL"
                                name="./rightSectionLink4URL"/>
                            <rightsectionlink5label
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Plese enter a label"
                                fieldLabel="Link 5 Label"
                                name="./rightSectionLink5Label"/>
                            <rightsectionlink5url
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Please enter a destination url"
                                fieldLabel="Link 5 URL"
                                name="./rightSectionLink5URL"/>
                            <rightsectionlink6label
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Plese enter a label"
                                fieldLabel="Link 6 Label"
                                name="./rightSectionLink6Label"/>
                            <rightsectionlink6url
                            	jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Please enter a destination url"
                                fieldLabel="Link 6 URL"
                                name="./rightSectionLink6URL"/>
                        </items>
                    </columns>
                </items>
            </rightsectioncolumn>
        </items>
    </content>
</jcr:root>
